import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class victorious{

    public static void main(String[] args) {
        Scanner rk = new Scanner(System.in);
        int r = rk.nextInt();
        for (int i=0;i<r;i++){
            char ch =(char)('A' + i);
            for (int j=0;j<=i;j++){
                System.out.print((char)(ch + j) + " ");
            }
            System.out.println();
        }
    }
}
        
