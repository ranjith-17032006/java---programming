import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class victorious {

    public static void main(String[] args) {
        Scanner rk = new Scanner(System.in);
        int a = rk.nextInt();
        int postive = 0;
        int negative = 0;
        for (int i=0;i<a;i++){
            int num =rk.nextInt();
            if(num > 0) {
                postive++;
            }else if(num < 0){
                negative++;
            }
        } System.out.printf("Count of Positive Integer is %.2f\n", (float)postive);
        System.out.printf("Count of Negative Integer is %.2f\n", (float)negative);
 }
}
