import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class victorious{

    public static void main(String[] args) {
        Scanner rk = new Scanner(System.in);
        int a = rk.nextInt();
        int sum=0;
        for(int i=0;i<a;i++){
            int value=rk.nextInt();
            sum+=value;
        }
        float mean=(float)sum/a;
        System.out.printf("Array Mean Value is %.2f",mean);
    }
}
    
